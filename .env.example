# Project Prism - Environment Variables Template
# Copy this file to .env and fill in your values

# Application Settings
APP_NAME=Project Prism
APP_ENV=development

# Database
# SQLite database path (relative to /app in container)
DB_PATH=/app/data/prism.db

# Logging
LOG_LEVEL=DEBUG
LOG_PATH=/app/logs/debug.log

# Authentication (for future use)
# AUTH_ENABLED=false
# AUTH_PROVIDER=google

# GCP Settings (for future integrations)
# GCP_PROJECT_ID=your-project-id
# GCP_REGION=us-central1
